try:
    num1,num2=eval(input("Input two numbers with a comma in the middle: "))
    result=num1*num2
    print("Result is ", result)

except ValueError:
    print("Invalid value")

except SyntaxError:
    print("Improper structure")

except:
    print("Invalid input")

else:
    print("No issues")

finally:
    print("There may or may not be a problem")
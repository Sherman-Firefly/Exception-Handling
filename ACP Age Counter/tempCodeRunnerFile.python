try:
    age=int(input("Input your age here: "))
    print("Age is", age)
    if age%2==0:
        print("Even age")
    elif age<0:
        print("You cannot have a negative age")
    else:
        print("Odd age")
    
except ValueError:
    print("Invalid value. Do not use decimals, letters, or any other character other than whole numbers")

except SyntaxError:
    print("Improper structure")

except:
    print("Invalid input")

else:
    print("No issues")
